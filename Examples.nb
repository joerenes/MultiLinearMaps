   RowBox[{"FromMatrix", "[", 
    RowBox[{
     RowBox[{"{", "A", "}"}], ",", 
     RowBox[{"{", "}"}], ",", 
     RowBox[{"Transpose", "[", 
      RowBox[{"{", 
       RowBox[{"{", 
        RowBox[{"\[Alpha]", ",", "\[Beta]"}], "}"}], "}"}], "]"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
